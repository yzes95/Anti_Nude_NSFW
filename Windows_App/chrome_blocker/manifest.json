{
  "manifest_version": 3,
  "name": "Tab NSFW Blocker (Local)",
  "version": "0.1",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["http://127.0.0.1:5000/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
